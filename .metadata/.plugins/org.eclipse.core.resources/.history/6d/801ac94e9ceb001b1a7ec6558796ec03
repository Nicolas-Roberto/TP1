package primeiroProjeto;


public class NovoCliente {
	static String nome;
	static String end;
	static String tel;
	
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getNome() {
		return nome;
	}
	public void setEnd(String end) {
		this.end = end;
	}
	public String getEnd() {
		return end;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	
}

package primeiroProjeto;

import java.io.FileWriter;
import java.io.PrintWriter;
import java.io.IOException;
import java.util.Scanner;

public class NovoCliente {
	static String nome;
	static String end;
	static String tel;
	
	public String setNome() {
		this.nome = nome
	}
	public void setEnd(String[] args) {
		try {
			NovoCliente.end = end;
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	public void setTel(String[] args) {
		try {
			NovoCliente.tel = tel;
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	

}
